		"#161821"
	],
	"vim.statusBarColors.searchinprogressmode": [
		"#818596",
		"#161821"
	],
	"vim.statusBarColors.surroundinputmode": [
		"#818596",
		"#161821"
	],
	"vim.statusBarColors.visual": [
		"#B4BE82",
		"#161821"
	],
	"vim.statusBarColors.visualblock": [
		"#B4BE82",
		"#161821"
	],
	"vim.statusBarColors.visualline": [
		"#B4BE82",
		"#161821"
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<space>"
			],
			"commands": [
				"vspacecode.space"
			]
		},
		{
			"before": [
				","
			],
			"commands": [
				"vspacecode.space",
				{
					"args": "m",
					"command": "whichkey.triggerKey"
				}
			]
		},
		{
			"before": [
				">"
			],
			"commands": [
				"editor.action.indentLines"
			]
		},
		{
			"before": [
				"<"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		}
	],
	"workbench.colorCustomizations": {
		// "statusBar.background": "#161821",
		// "statusBar.debuggingBackground": "#161821",
		// "statusBar.debuggingForeground": "#818596",
		// "statusBar.foreground": "#818596",
		// "statusBar.noFolderBackground": "#161821",
		"[Catppuccin Latte]": {
			"editorInlayHint.typeBackground": "#ccd0da",
			"editorInlayHint.typeForeground": "#5c5f77"
		},
		"[Catppuccin Macchiato]": {
			"editorInlayHint.typeBackground": "#1e2030",
			"editorInlayHint.typeForeground": "#a5adcb"
		},
	},
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"yaml.customTags": [
		"!And",
		"!And sequence",
		"!If",
		"!If sequence",
		"!Not",
