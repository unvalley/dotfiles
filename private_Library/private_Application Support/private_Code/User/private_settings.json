	],
	"color-highlight.languages": [
		"*"
	],
	"debug.allowBreakpointsEverywhere": true,
	"debug.inlineValues": "on",
	"debug.internalConsoleOptions": "openOnSessionStart",
	"debug.openDebug": "openOnSessionStart",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.accessibilitySupport": "off",
	"editor.autoClosingBrackets": "always",
	"editor.autoClosingQuotes": "always",
	"editor.autoSurround": "languageDefined",
	"editor.cursorSmoothCaretAnimation": "off",
	"editor.cursorStyle": "line",
	"editor.detectIndentation": false,
	"editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
	"editor.fontLigatures": false,
	"editor.fontSize": 16,
	"editor.formatOnPaste": false,
	"editor.formatOnSave": true,
	"editor.formatOnType": false,
	"editor.inlineSuggest.enabled": true,
	"editor.insertSpaces": true,
	"editor.lightbulb.enabled": false,
	"editor.lineNumbers": "on",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.parameterHints.enabled": true,
	"editor.renderWhitespace": "all",
	"editor.rulers": [
		80
	],
	"editor.scrollBeyondLastLine": false,
	"editor.showFoldingControls": "always",
	"editor.snippetSuggestions": "top",
	"editor.stickyScroll.enabled": true,
	"editor.suggest.localityBonus": true,
	"editor.suggestSelection": "first",
	"editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
	"editor.wordWrap": "off",
	"emmet.triggerExpansionOnTab": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"exsql.inMemory.executor.cache_all_fields.enabled": true,
	"exsql.inMemory.executor.enabled": true,
	"files.associations": {
		"*.md": "mdx",
		"*.tsx": "typescriptreact",
		"*.vue": "vue"
	},
	"files.autoSave": "onFocusChange",
	"files.eol": "\n",
	"files.exclude": {
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.project": true,
		"**/.settings": true,
		"/.git": true,
		"/node_modules": true,
		"/node_modules/": true,
		"/target/": true,
		"USE_GITIGNORE": true
	},
	"files.trimTrailingWhitespace": true,
	"files.watcherExclude": {
		"**/.ammonite": true,
		"**/.bloop": true,
		"**/.metals": true,
		"/node_modules": true,
		"/target": true
	},
	"git.autofetch": true,
	"github.copilot.enable": {
		"*": true,
		"markdown": true,
		"plaintext": true,
		"rust": true,
		"typescript": true,
		"yaml": true
	},
	"githubPullRequests.pullBranch": "never",
	"gitlens.codeLens.authors.command": false,
	"gitlens.codeLens.authors.enabled": false,
	"gitlens.hovers.currentLine.over": "line",
	"go.autocompleteUnimportedPackages": true,
	"go.docsTool": "godoc",
	"go.formatFlags": [
		"-w"
	],
	"go.formatTool": "goimports",
	"go.inlayHints.assignVariableTypes": true,
	"go.languageServerExperimentalFeatures": {
		"diagnostics": true
	},
	"go.lintFlags": [
		"--enable-all",
		"--new"
	],
	"go.toolsManagement.autoUpdate": true,
	"go.useLanguageServer": true,
	"gopls": {
		"usePlaceholders": true
	},
	"haskell.manageHLS": "GHCup",
	"html.format.contentUnformatted": "pre, code, textarea, title, h1, h2, h3, h4, h5, h6, p",
	"html.format.extraLiners": "",
	"html.format.unformatted": null,
	"indentRainbow.excludedLanguages": [
		"vue"
	],
	"leetcode.defaultLanguage": "rust",
	"leetcode.filePath": {
		"default": {
			"filename": "${id}.${kebab-case-name}.${ext}",
			"folder": ""
		}
	},
	"mssql.connections": [
		{
			"database": "{{put-database-name-here}}",
			"password": "",
			"server": "{{put-server-name-here}}",
			"user": "{{put-username-here}}"
		}
	],
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"npm.keybindingsChangedWarningShown": true,
	"prettier.requireConfig": false,
	"redhat.telemetry.enabled": true,
	"remote.onstartup": true,
	"screencastMode.onlyKeyboardShortcuts": true,
	"search.exclude": {
		"**/*.code-search": true,
		"**/.build": true,
		"**/.gh-pages": true,
		"**/bower_components": true,
		"**/dist": true,
		"**/node_modules": true,
		"/node_modules": true,
		"/target": true
	},
	"sync.removeExtensions": true,
	"tabnine.experimentalAutoImports": true,
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.defaultProfile.osx": "fish",
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"terminal.integrated.fontFamily": "Hack Nerd Font Mono",
	"terminal.integrated.fontSize": 15,
	"terminal.integrated.inheritEnv": false,
	"terminal.integrated.rightClickBehavior": "paste",
	"todo-tree.tree.showScanModeButton": false,
	"update.mode": "manual",
	"vim.autoSwitchInputMethod.enable": true,
	"vim.debug.silent": true,
	"vim.easymotion": true,
	"vim.easymotionKeys": "sdfghjkl",
	"vim.foldfix": true,
	"vim.highlightedyank.color": "rgba(0, 240, 170, 0.5)",
	"vim.highlightedyank.duration": 100,
	"vim.highlightedyank.enable": true,
	"vim.hlsearch": true,
	"vim.leader": "<space>",
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"after": [
				"leader",
				"leader",
				"leader",
				"b",
				"d",
				"w"
			],
			"before": [
				"shift+f"
			]
		},
		{
			"before": [
				"alt+s"
			],
			"commands": [
				":split"
			]
		},
		{
			"before": [
				"alt+v"
			],
			"commands": [
				":vsplit"
			]
		},
		{
			"before": [
				"<space>"
			],
			"commands": [
				"vspacecode.space"
			]
		},
		{
			"before": [
				","
			],
			"commands": [
				"vspacecode.space",
				{
					"args": "m",
					"command": "whichkey.triggerKey"
				}
			]
		}
	],
	"vim.searchHighlightColor": "rgba(255, 235, 60, 0.4)",
	"vim.sneak": true,
