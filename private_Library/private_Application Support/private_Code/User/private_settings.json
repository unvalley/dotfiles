{
    "sortJSON.orderOverride": [
        "sortJSON.orderOverride"
    ],
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 15,
    "vim.highlightedyank.duration": 100,
    "aws.samcli.location": "/usr/local/bin/sam",
    "debug.inlineValues": "on",
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.openDebug": "openOnSessionStart",
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.autoSurround": "languageDefined",
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.cursorStyle": "line",
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.lineNumbers": "on",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "first",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "githubPullRequests.pullBranch": "never",
    "gitlens.hovers.currentLine.over": "line",
    "go.docsTool": "godoc",
    "go.formatTool": "goimports",
    "haskell.manageHLS": "GHCup",
    "html.format.contentUnformatted": "pre, code, textarea, title, h1, h2, h3, h4, h5, h6, p",
    "html.format.extraLiners": "",
    "indentRainbow.indicatorStyle": "light",
    "leetcode.defaultLanguage": "rust",
    "terminal.integrated.defaultProfile.osx": "fish",
    "terminal.integrated.fontFamily": "Hack Nerd Font Mono",
    "terminal.integrated.rightClickBehavior": "paste",
    "update.mode": "manual",
    "vim.easymotionKeys": "sdfghjkl",
    "vim.highlightedyank.color": "rgba(0, 240, 170, 0.5)",
    "vim.leader": "<space>",
    "vim.searchHighlightColor": "rgba(255, 235, 60, 0.4)",
    "vim.whichwrap": "h,l,<,>,[,]",
    "window.openFilesInNewWindow": "off",
    "workbench.colorTheme": "GitHub Light",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.preferredDarkColorTheme": "GitHub Dark Default",
    "workbench.preferredLightColorTheme": "GitHub Light",
    "workbench.tree.renderIndentGuides": "always",
    "[csv]": {
        "editor.wordWrap": "off"
    },
    "[deno]": {
        "deno.enable": true,
        "deno.inlayHints.enumMemberValues.enabled": true,
        "deno.inlayHints.functionLikeReturnTypes.enabled": true,
        "deno.inlayHints.parameterTypes.enabled": true,
        "deno.inlayHints.variableTypes.enabled": true
    },
    "[go]": {},
    "[haskell]": {
        "editor.defaultFormatter": "haskell.haskell"
    },
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.codeActionsOnSave": {
            "source.addMissingImports": false,
            "source.fixAll.eslint": true,
            "source.organizeImports": true
        }
    },
    "[json]": {
        "editor.formatOnSave": false
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.formatOnSave": false,
        "editor.trimAutoWhitespace": false,
        "files.trimTrailingWhitespace": false
    },
    "[mdx]": {
        "editor.formatOnSave": false
    },
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "rust-analyzer.checkOnSave.command": "clippy",
        "rust-analyzer.rustc.source": "discover",
        "rust-analyzer.linkedProjects": [
            "Cargo.toml",
            "clippy_dev/Cargo.toml",
            "lintcheck/Cargo.toml"
        ],
        "editor.formatOnSave": true,
        "rust-analyzer.procMacro.enable": false
    },
    "[scala]": {
        "editor.tabSize": 2
    },
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[terraform]": {
        "editor.defaultFormatter": "hashicorp.terraform"
    },
    "[tsv]": {
        "editor.wordWrap": "off"
    },
    "[typescript]": {
        "editor.autoClosingBrackets": "always",
        "editor.defaultFormatter": "biomejs.biome",
        "editor.suggestSelection": "recentlyUsed",
        "editor.codeActionsOnSave": {
            "source.addMissingImports": true,
            "source.fixAll.eslint": true,
            "source.organizeImports": false
        },
        "editor.suggest.showKeywords": true
    },
    "[typescriptreact]": {
        "editor.autoClosingBrackets": "languageDefined",
        "editor.defaultFormatter": "biomejs.biome",
        "editor.suggestSelection": "recentlyUsed",
        "editor.codeActionsOnSave": {
            "source.addMissingImports": true,
            "source.fixAll.eslint": true,
            "source.organizeImports": false
        },
        "editor.suggest.showKeywords": false
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascriptreact",
        "typescriptreact",
        "plaintext",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (Eex)"
    ],
    "color-highlight.languages": [
        "*"
    ],
    "cSpell.userWords": [
        "Apks",
        "Appolo",
        "Bignumeric",
        "binded",
        "blockquote",
        "Bpaf",
        "chronotc",
        "consts",
        "COUNTIF",
        "datatypes",
        "DBIO",
        "decompile",
        "deno",
        "denoland",
        "Deque",
        "dotd",
        "dtos",
        "exsql",
        "gcloud",
        "grammer",
        "hasher",
        "Iframe",
        "instantiator",
        "Invisibles",
        "Itertools",
        "Jdbc",
        "lalrpop",
        "losslessly",
        "lossly",
        "luxon",
        "millis",
        "mrkdwn",
        "myshoes",
        "Newtypes",
        "Nodata",
        "Nonoctal",
        "Onwer",
        "peekable",
        "pico",
        "protobuf",
        "QUANTILES",
        "redeclaration",
        "rfind",
        "rustc",
        "scalafmt",
        "schemars",
        "subquery",
        "Tranzact",
        "typst",
        "UNNEST",
        "unvalley",
        "Usecase"
    ],
    "editor.rulers": [
        80
    ],
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "files.associations": {
        "*.md": "mdx",
        "*.tsx": "typescriptreact",
        "*.vue": "vue"
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "/.git": true,
        "/node_modules": true,
        "/node_modules/": true,
        "/target/": true,
        "USE_GITIGNORE": true
    },
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true,
        "/node_modules": true,
        "/target": true
    },
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": true,
        "rust": true,
        "typescript": true,
        "yaml": true
    },
    "go.formatFlags": [
        "-w"
    ],
    "go.languageServerExperimentalFeatures": {
        "diagnostics": true
    },
    "go.lintFlags": [
        "--enable-all",
        "--new"
    ],
    "gopls": {
        "usePlaceholders": true
    },
    "indentRainbow.excludedLanguages": [
        "vue"
    ],
    "leetcode.filePath": {
        "default": {
            "filename": "${id}.${kebab-case-name}.${ext}",
            "folder": ""
        }
    },
    "mssql.connections": [
        {
            "database": "{{put-database-name-here}}",
            "password": "",
            "server": "{{put-server-name-here}}",
            "user": "{{put-username-here}}"
        }
    ],
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "search.exclude": {
        "**/.build": true,
        "**/.gh-pages": true,
        "**/*.code-search": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/node_modules": true,
        "/node_modules": true,
        "/target": true
    },
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "after": [
                "leader",
                "leader",
                "leader",
                "b",
                "d",
                "w"
            ],
            "before": [
                "shift+f"
            ]
        },
        {
            "before": [
                "alt+s"
            ],
            "commands": [
                ":split"
            ]
        },
        {
            "before": [
                "alt+v"
            ],
            "commands": [
                ":vsplit"
            ]
        },
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "args": "m",
                    "command": "whichkey.triggerKey"
                }
            ]
        }
    ],
    "vim.statusBarColors.commandlineinprogress": [
        "#818596",
        "#161821"
    ],
    "vim.statusBarColors.easymotioninputmode": [
