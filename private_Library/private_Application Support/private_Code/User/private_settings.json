{
	// Debug
	"debug.inlineValues": "on",
	"debug.internalConsoleOptions": "openOnSessionStart",
	"debug.openDebug": "openOnSessionStart",
	// Editor
	"editor.fontSize": 16,
	"editor.accessibilitySupport": "off",
	"editor.autoClosingBrackets": "always",
	"editor.autoClosingQuotes": "always",
	"editor.autoSurround": "languageDefined",
	"editor.cursorSmoothCaretAnimation": "off",
	"editor.cursorStyle": "line",
	"editor.fontFamily": "Hack Nerd Font Mono, monospace",
	"editor.lineNumbers": "on",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.renderWhitespace": "all",
	"editor.showFoldingControls": "always",
	"editor.snippetSuggestions": "top",
	"editor.suggestSelection": "first",
	"editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
	"editor.wordWrap": "off",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.detectIndentation": false,
	"editor.fontLigatures": false,
	"editor.formatOnPaste": false,
	"editor.formatOnSave": false,
	"editor.formatOnType": false,
	"editor.inlineSuggest.enabled": true,
	"editor.insertSpaces": true,
	"editor.lightbulb.enabled": "off",
	"editor.parameterHints.enabled": true,
	"editor.scrollBeyondLastLine": false,
	"editor.stickyScroll.enabled": true,
	"editor.suggest.localityBonus": true,
	"editor.rulers": [
		80
	],
	// Terminal
	"terminal.integrated.fontSize": 15,
	"terminal.integrated.defaultProfile.osx": "fish",
	"terminal.integrated.fontFamily": "Hack Nerd Font Mono, monospace",
	"terminal.integrated.rightClickBehavior": "paste",
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"terminal.integrated.inheritEnv": false,
	// Files
	"files.autoSave": "onFocusChange",
	"files.eol": "\n",
	"files.associations": {
		"*.tsx": "typescriptreact",
		"*.vue": "vue"
	},
	"files.exclude": {
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.project": true,
		"**/.settings": true,
		"/.git": true,
		"/node_modules": true,
		"/node_modules/": true,
		"/target/": true,
		"USE_GITIGNORE": true
	},
	"files.watcherExclude": {
		"**/.ammonite": true,
		"**/.bloop": true,
		"**/.metals": true,
		"/node_modules": true,
		"/target": true
	},
	"githubPullRequests.pullBranch": "never",
	"gitlens.hovers.currentLine.over": "line",
	// Go
	"go.docsTool": "godoc",
	"go.formatTool": "goimports",
	"go.autocompleteUnimportedPackages": true,
	"go.inlayHints.assignVariableTypes": true,
	"go.toolsManagement.autoUpdate": true,
	"go.useLanguageServer": true,
	"html.format.contentUnformatted": "pre, code, textarea, title, h1, h2, h3, h4, h5, h6, p",
	"html.format.extraLiners": "",
	"indentRainbow.indicatorStyle": "light",
	"leetcode.defaultLanguage": "rust",
	"update.mode": "manual",
	"vim.highlightedyank.duration": 100,
	"vim.easymotionKeys": "sdfghjkl",
	"vim.highlightedyank.color": "rgba(0, 240, 170, 0.5)",
	"vim.leader": "<space>",
	"vim.searchHighlightColor": "rgba(255, 235, 60, 0.4)",
	"vim.whichwrap": "h,l,<,>,[,]",
	// Workbench
	"workbench.editor.showTabs": "multiple",
	"workbench.preferredDarkColorTheme": "Catppuccin Macchiato",
	"workbench.preferredLightColorTheme": "Catppuccin Latte",
	"workbench.tree.renderIndentGuides": "always",
	// Window
	"window.openFilesInNewWindow": "off",
	"window.autoDetectColorScheme": true,
	"window.commandCenter": true,
	"[csv]": {
		"editor.wordWrap": "off"
	},
	"[deno]": {
		"deno.enable": true,
		"deno.inlayHints.enumMemberValues.enabled": true,
		"deno.inlayHints.functionLikeReturnTypes.enabled": true,
		"deno.inlayHints.parameterTypes.enabled": true,
		"deno.inlayHints.variableTypes.enabled": true
	},
	"[go]": {},
	"go.formatFlags": [
		"-w"
	],
	"go.languageServerExperimentalFeatures": {
		"diagnostics": true
	},
	"go.lintFlags": [
		"--enable-all",
		"--new"
	],
	"[haskell]": {
		"editor.defaultFormatter": "haskell.haskell",
		"haskell.manageHLS": "GHCup",
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.codeActionsOnSave": {
			"source.addMissingImports": "never",
			"source.fixAll.eslint": "explicit",
			"source.organizeImports": "explicit"
		}
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[markdown]": {
		"editor.wordWrap": "on",
		"editor.formatOnSave": false,
		"editor.trimAutoWhitespace": false,
		"files.trimTrailingWhitespace": false
	},
	"[mdx]": {
		"editor.formatOnSave": false,
		"files.autoSave": "off",
		"editor.trimAutoWhitespace": false,
		"files.trimTrailingWhitespace": false
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.formatOnSave": true,
		"rust-analyzer.checkOnSave.command": "clippy",
		"rust-analyzer.rustc.source": "discover",
		"rust-analyzer.linkedProjects": [
			"Cargo.toml",
			"clippy_dev/Cargo.toml",
			"lintcheck/Cargo.toml"
		],
	},
	"rust-analyzer.procMacro.enable": true,
    "rust-analyzer.procMacro.ignored": { "napi-derive": ["napi"] },
    // "rust-analyzer.diagnostics.disabled": ["macro-error"],
	"rust-analyzer.cargo.features": "all",
	"[scala]": {
		"editor.tabSize": 2
	},
	"[sql]": {
		"editor.defaultFormatter": "mtxr.sqltools"
	},
	"[terraform]": {
		"editor.defaultFormatter": "hashicorp.terraform"
	},
	"[tsv]": {
		"editor.wordWrap": "off"
	},
	"[typescript]": {
		"editor.autoClosingBrackets": "always",
		"editor.defaultFormatter": "biomejs.biome",
		"editor.suggestSelection": "recentlyUsed",
		"editor.codeActionsOnSave": {
			"source.addMissingImports": "explicit",
			"source.fixAll.eslint": "explicit",
			"source.organizeImports": "never"
		},
		"editor.suggest.showKeywords": true,
		"editor.formatOnSave": true,
		"editor.inlayHints.enabled": "on"
	},
	"[typescriptreact]": {
		"editor.autoClosingBrackets": "languageDefined",
		"editor.defaultFormatter": "biomejs.biome",
		"editor.suggestSelection": "recentlyUsed",
		"editor.codeActionsOnSave": {
			"source.addMissingImports": "explicit",
			"source.fixAll.eslint": "explicit",
			"source.organizeImports": "never"
		},
		"editor.suggest.showKeywords": false,
		"editor.formatOnSave": true,
		"editor.inlayHints.enabled": "on"
	},
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[plaintext]": {
		"editor.formatOnSave": false
	},
	"auto-close-tag.activationOnLanguage": [
		"xml",
		"php",
		"blade",
		"ejs",
		"jinja",
		"javascriptreact",
		"typescriptreact",
		"plaintext",
		"vue",
		"liquid",
		"erb",
		"lang-cfml",
		"cfml",
		"HTML (Eex)"
	],
	"color-highlight.languages": [
		"*"
	],
