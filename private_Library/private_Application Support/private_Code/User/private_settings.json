	"github.copilot.enable": {
		"*": true,
		"markdown": true,
		"plaintext": true,
		"rust": true,
		"typescript": true,
		"yaml": true
	},
	"gopls": {
		"usePlaceholders": true
	},
	"indentRainbow.excludedLanguages": [
		"vue"
	],
	"leetcode.filePath": {
		"default": {
			"filename": "${id}.${kebab-case-name}.${ext}",
			"folder": ""
		}
	},
	"mssql.connections": [
		{
			"database": "{{put-database-name-here}}",
			"password": "",
			"server": "{{put-server-name-here}}",
			"user": "{{put-username-here}}"
		}
	],
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"search.exclude": {
		"**/.build": true,
		"**/.gh-pages": true,
		"**/*.code-search": true,
		"**/bower_components": true,
		"**/dist": true,
		"**/node_modules": true,
		"/node_modules": true,
		"/target": true
	},
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"after": [
				"leader",
				"leader",
				"leader",
				"b",
				"d",
				"w"
			],
			"before": [
				"shift+f"
			]
		},
		{
			"before": [
				"alt+s"
			],
			"commands": [
				":split"
			]
		},
		{
			"before": [
				"alt+v"
			],
			"commands": [
				":vsplit"
			]
		},
		{
			"before": [
				"<space>"
			],
			"commands": [
				"vspacecode.space"
			]
		},
		{
			"before": [
				","
			],
			"commands": [
				"vspacecode.space",
				{
					"args": "m",
					"command": "whichkey.triggerKey"
				}
			]
		}
	],
