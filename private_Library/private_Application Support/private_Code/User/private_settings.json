{
	"sortJSON.orderOverride": [
		"sortJSON.orderOverride"
	],
	"[csv]": {
		"editor.wordWrap": "off"
	},
	"[deno]": {
		"deno.enable": true,
		"deno.inlayHints.enumMemberValues.enabled": true,
		"deno.inlayHints.functionLikeReturnTypes.enabled": true,
		"deno.inlayHints.parameterTypes.enabled": true,
		"deno.inlayHints.variableTypes.enabled": true
	},
	"[go]": {},
	"[haskell]": {
		"editor.defaultFormatter": "haskell.haskell"
	},
	"[javascript]": {
		"editor.codeActionsOnSave": {
			"source.addMissingImports": false,
			"source.fixAll.eslint": true,
			"source.organizeImports": true
		},
		"editor.defaultFormatter": "rome.rome"
	},
	"[json]": {
		"editor.formatOnSave": false
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[markdown]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.wordWrap": "on",
		"editor.trimAutoWhitespace": false,
		"files.trimTrailingWhitespace": false
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"rust-analyzer.checkOnSave.command": "clippy",
		"rust-analyzer.linkedProjects": [
			"Cargo.toml",
			"clippy_dev/Cargo.toml",
			"lintcheck/Cargo.toml"
		],
		"rust-analyzer.procMacro.enable": false,
		"rust-analyzer.rustc.source": "discover"
	},
	"[scala]": {
		"editor.tabSize": 2
	},
	"[sql]": {
		"editor.defaultFormatter": "mtxr.sqltools"
	},
	"[terraform]": {
		"editor.defaultFormatter": "hashicorp.terraform"
	},
	"[tsv]": {
		"editor.wordWrap": "off"
	},
	"[typescript]": {
		"editor.autoClosingBrackets": "always",
		"editor.codeActionsOnSave": {
			"source.addMissingImports": true,
			"source.fixAll.eslint": true,
			"source.organizeImports": false
		},
		"editor.defaultFormatter": "rome.rome",
		"editor.suggest.showKeywords": true,
		"editor.suggestSelection": "recentlyUsed"
	},
	"[typescriptreact]": {
		"editor.autoClosingBrackets": "languageDefined",
		"editor.codeActionsOnSave": {
			"source.addMissingImports": true,
			"source.fixAll.eslint": true,
			"source.organizeImports": false
		},
		"editor.suggest.showKeywords": false,
		"editor.suggestSelection": "recentlyUsed"
	},
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"auto-close-tag.activationOnLanguage": [
		"xml",
		"php",
		"blade",
		"ejs",
		"jinja",
		"javascriptreact",
		"typescriptreact",
		"plaintext",
		"vue",
		"liquid",
		"erb",
		"lang-cfml",
		"cfml",
		"HTML (Eex)"
	],
	"aws.samcli.location": "/usr/local/bin/sam",
	"aws.telemetry": false,
	"bracketPairColorizer.depreciation-notice": false,
	"cSpell.userWords": [
		"Apks",
		"Appolo",
		"Bignumeric",
		"blockquote",
		"Bpaf",
		"chronotc",
		"COUNTIF",
		"datatypes",
